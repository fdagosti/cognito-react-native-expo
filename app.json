{
  "expo": {
    "name": "react-native-aws-cognito-expo",
    "description": "A sample application to show AWS Cognito in a react-native application.",
    "slug": "reactnativeawscognitoexpo",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": true
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactnativeawscognitoexpo.voting"
    },
    "android": {
      "package": "com.reactnativeawscognitoexpo.voting"
    }
  }
}